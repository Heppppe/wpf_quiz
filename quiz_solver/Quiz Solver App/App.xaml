<Application x:Class="Quiz_Solver_App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Quiz_Solver_App"
             xmlns:vm="clr-namespace:Quiz_Solver_App.ViewModel"
             xmlns:views="clr-namespace:Quiz_Solver_App.View"
             xmlns:System="clr-namespace:System;assembly=mscorlib">
    
    <Application.Resources>
        <ResourceDictionary>


            <DataTemplate DataType="{x:Type vm:MainMenuViewModel}">
                <views:MainMenu/>
            </DataTemplate>

            <DataTemplate DataType="{x:Type vm:QuizSolverViewModel}">
                <views:QuizSolver/>
            </DataTemplate>

        </ResourceDictionary>
    </Application.Resources>
</Application>
