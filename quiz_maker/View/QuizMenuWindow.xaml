<Window x:Class="quiz_maker.View.QuizMenuWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:quiz_maker.View"
        mc:Ignorable="d"
        Title="QuizMenuWindow" Height="450" Width="800">
    <Grid>
        <ListBox ItemsSource="{Binding Quizzes}" SelectedItem="{Binding SelectedQuiz}">
        </ListBox>
        <Button Content="Edit" Command="{Binding EditQuizCommand}" CommandParameter="{Binding SelectedQuiz}" />

    </Grid>
</Window>
